{
  "name": "nj-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.34",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build:client": "webpack --config src/build/webpack.config.client.js",
    "build:server": "webpack --config src/build/webpack.config.server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config src/build/webpack.config.client.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "node server/server.js"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  }
}
